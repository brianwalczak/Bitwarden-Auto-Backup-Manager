{
    "name": "bitwarden-auto-backup-manager",
    "productName": "Bitwarden Auto-Backup Manager",
    "description": "A robust application that creates local, encrypted auto-backups for your Bitwarden vault.",
    "author": "Brian Walczak",
    "version": "2.2.1",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "build": {
        "appId": "com.brianwalczak.bitwarden-auto-backup-manager",
        "win": {
            "target": "nsis"
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": false,
            "perMachine": false
        },
        "extraResources": [
            {
                "from": "build/icon.ico",
                "to": "icon.ico"
            }
        ]
    },
    "dependencies": {
        "@bitwarden/sdk-internal": "0.2.0-main.546",
        "big-integer": "1.6.52",
        "electron-log": "^5.4.3",
        "keytar": "^7.9.0",
        "rxjs": "7.8.1",
        "tldts": "7.0.22"
    },
    "devDependencies": {
        "electron": "^40.4.1",
        "electron-builder": "^26.8.1",
        "prettier": "^3.8.1"
    }
}
